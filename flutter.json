{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "groupName": "Flutter SDK",
      "matchPaths": [".fvm/fvm_config.json", "pubspec.yaml"],
      "matchPackageNames": [
        "flutter",
        "material_color_utilities",
        "collection"
      ],
      "labels": ["fluttersdk"],
      "semanticCommitScope": "flutter",
      "automerge": false
    }
  ]
}
